{
  "ConnectionStrings:Headhunter": "Server=localhost;Database=Headhunter;Trusted_Connection=True;Encrypt=False;",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "Enrich": [ "FromLogContext" ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.EntityFrameworkCore": "Verbose"
      }
    },
    "WriteTo": [
      "Console"
    ]
  },
  "AllowedHosts": "*"
}
